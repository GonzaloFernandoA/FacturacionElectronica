<appender name="AP_FE" type="ZooLogicSA.Core.Logueo.ZooRollingfileappender">
  <file value="Fe.log"/>
  <appendToFile value="true"/>
  <maximumFileSize value="2048KB"/>
  <rollingStyle value="size"/>
  <maxSizeRollBackups value="10"/>
  <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
  <layout type="log4net.Layout.PatternLayout">
    <conversionPattern value="%date{dd/MM/yyyy}, Base: %property{BaseDatos}, Versi&#243;n: %property{Version}, %newline            Nombre de la PC: %property{NombrePc}, Usuario de la PC: %property{UsuarioPc}%newline %message %newline"/>
  </layout>
</appender>
<appender name="AP_FESINCABECERA" type="ZooLogicSA.Core.Logueo.ZooRollingfileappender">
  <file value="Fe.log"/>
  <appendToFile value="true"/>
  <maximumFileSize value="2048KB"/>
  <rollingStyle value="size"/>
  <maxSizeRollBackups value="10"/>
  <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
  <layout type="log4net.Layout.PatternLayout">
    <conversionPattern value=" %message %newline"/>
  </layout>
</appender>